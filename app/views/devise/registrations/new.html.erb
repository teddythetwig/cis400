<!-- Navbar -->
<div class="navbar navbar-fixed-top">
  <div class="navbar-inner">
    <div class="container-fluid">
      <%= link_to 'SIQL', '/', :class => 'brand' %>
    </div> <!--/.container-fluid -->
  </div> <!--/.navbar-inner -->
</div> <!--/.navbar-fixed-top -->
<div class="container">

  <div class="page-header">
    <h2>Sign up</h2>
  </div>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div class="control-group">
    <%= f.label :email, :class => "control-label", :for => "emailInput" %>
    <div class="controls">
      <%= f.email_field :email, :class => "input-xlarge focused", :id => "emailInput" %>
    </div>
  </div>

  <div class="control-group">
    <%= f.label :password, :class => "control-label", :for => "pwordInput" %>
    <div class="controls">
      <%= f.password_field :password, :class => "input-xlarge focused", :id => "pwordInput" %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :password_confirmation, :class => "control-label", :for => "ppwordInput" %>
    <div class="controls">
      <%= f.password_field :password_confirmation, :class => "input-xlarge focused", :id => "ppwordInput" %>
    </div>
  </div>

  <div class="modal-footer">
    <%= f.submit "Sign up", :class => 'btn btn-success' %>
  </div>
<% end %>

<%= render "devise/shared/links" %>

</div>
